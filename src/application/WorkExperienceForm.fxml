<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.WorkExperienceController">
   <children>
      <TextField fx:id="txtEmployer" layoutX="38.0" layoutY="87.0" opacity="0.54" text="Company Name">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <TextField fx:id="txtTitle" layoutX="38.0" layoutY="136.0" opacity="0.54" text="Position">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <TextField fx:id="txtDuration" layoutX="38.0" layoutY="185.0" opacity="0.54" text="Duration">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Button fx:id="btnExp" cancelButton="true" layoutX="111.0" layoutY="241.0" mnemonicParsing="false" onAction="#addExperience" text="Submit">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label fx:id="lblExperience" alignment="CENTER" layoutX="24.0" layoutY="39.0" prefHeight="17.0" prefWidth="250.0" text="Add Work Experience" textFill="#d66400">
