<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="683.0" prefWidth="782.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ReminderController">
   <children>
      <TextField fx:id="txtDate" layoutX="38.0" layoutY="131.0" opacity="0.54" text="Date">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <TextField fx:id="txtTime" layoutX="38.0" layoutY="190.0" opacity="0.54" text="Time">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <TextField fx:id="txtNotes" layoutX="38.0" layoutY="248.0" opacity="0.54" text="Email">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Button fx:id="btnSubmit" cancelButton="true" layoutX="110.0" layoutY="307.0" mnemonicParsing="false" onAction="#addReminder" text="Submit">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label fx:id="lblContact" alignment="CENTER" layoutX="24.0" layoutY="39.0" prefHeight="17.0" prefWidth="250.0" text="Add Reminder" textFill="#d66400">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <ComboBox fx:id="reminderBox" editable="true" layoutX="110.0" layoutY="86.0" prefWidth="150.0" promptText="Select Application" />
   </children>
</AnchorPane>
